{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
        "@services/*": ["app/shared/services/*"],
        "@models/*": ["app/shared/models/*"],
        "@helpers/*": ["app/shared/helpers/*"],
        "@store/*": ["app/shared/ngrx/*"],
        "@pipes/*": ["app/shared/pipes/*"],
        "@wtw/*": ["app/*"]
    },
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2020",
      "dom"
    ]
  }
}
